{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.vercel.app https://*.trustloan.app https://secure.walletconnect.org https://secure-mobile.walletconnect.org;"
        }
      ]
    }
  ]
}
